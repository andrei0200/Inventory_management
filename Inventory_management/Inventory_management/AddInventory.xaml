        <TextBox x:Name="in_pret" HorizontalAlignment="Left" Margin="138,288,0,0" Text="" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" FontSize="16" FontWeight="Bold" Height="30"/>
        <Image Margin="9,-2,24,246" Stretch="Fill" Source="image2.png"/>
        <Image HorizontalAlignment="Left" Height="187" Margin="401,161,0,0" VerticalAlignment="Top" Width="143" Source="/addd.png"/>
